{
  "compilerOptions": {
    "target": "es2018",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "baseUrl": "..",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "resolveJsonModule": true,
    "types": [
      "unlighthouse",
      "unlighthouse/nuxt.d.ts",
      "unlighthouse/nuxt",
      "@nuxt/types"
    ],
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true
  },
  "references": [
    {
      "path": "../../"
    }
  ],
  "include": [
    "node_modules/unlighthouse/nuxt.d.ts"
  ]
}
